// Ask user to swap 
#include <stdio.h>
int main() {
	int num1, num2, temp;
	printf("Enter the first number: ");
	scanf("%d", &num1);
	printf("Enter the second number: ");
	scanf("%d", &num2);
	temp = num1;
	num1 = num2;
	num2 = temp;
	printf("\nAfter swapping:-\n");
	printf("first number = %d\n", num1);
	printf("second number = %d\n", num2);
	return 0;
}
-----------------------------------------------------------

Input 1:
3 5
Output 1:
After swap: 5 3

Input 2:
-1 1
Output 2:
After swap: 1 -1

*/
