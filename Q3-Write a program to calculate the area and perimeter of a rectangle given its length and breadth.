#include <stdio.h>

int main() {
	int length, breadth, area , perimeter;
	    length = 5 , breadth = 10;
	    
	 area = length * breadth;
	 perimeter = 2*(length + breadth);
	 
	     printf("Length = %d\n",length);
	     printf("Breadth = %d\n",breadth);
	     printf("Area = %d\n",area);
	     printf("Perimeter = %d\n",perimeter);
	     
	return 0;
}
----------------------------------------------------------
Sample Test Cases:
Input 1:
5 10
Output 1:
Area=50, Perimeter=30

Input 2:
3 7
Output 2:
Area=21, Perimeter=20

*/
