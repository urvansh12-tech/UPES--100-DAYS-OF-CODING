#include <stdio.h>

int main() {
    int num1, num2;
    
    scanf("%d %d", &num1, &num2);

    printf("Sum=%d, Diff=%d, Product=%d, Quotient=%d\n", 
           num1 + num2, num1 - num2, num1 * num2, num1 / num2);

    return 0;
}
---------------------------------------------------------------------------------

Input 1:
num1 = 10, num = 2
Output 1:
Sum=12, Diff=8, Product=20, Quotient=5

Input 2:
num = 7, num = 3
Output 2:
Sum=10, Diff=4, Product=21, Quotient=2
